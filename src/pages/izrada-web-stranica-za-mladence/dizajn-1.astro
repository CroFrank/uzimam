  <!-- start of wpo-contact-section -->
  <section class="wpo-contact-section-s2 section-padding pt-0" id="rsvp">
    <div class="container">
      <div class="contact-wrap">
        <div class="wpo-contact-section-wrapper">
          <div class="wpo-contact-form-area">
            <div class="wpo-section-title">
              <h2>Are You Attending?</h2>
            </div>
            <form
              method="post"
              class="contact-validation-active"
              id="contact-form-main"
            >
              <div>
                <input
                  type="text"
                  class="form-control"
                  name="name"
                  id="name"
                  placeholder="Name"
                />
              </div>
              <div>
                <input
                  type="email"
                  class="form-control"
                  name="email"
                  id="email"
                  placeholder="Email"
                />
              </div>
              <div class="radio-buttons">
                <p>
                  <input type="radio" id="attend" name="radio-group" checked />
                  <label for="attend">Yes, I will be there</label>
                </p>
                <p>
                  <input type="radio" id="not" name="radio-group" />
                  <label for="not">Sorry, I can’t come</label>
                </p>
              </div>
              <div>
                <input
                  type="text"
                  class="form-control"
                  name="with-you"
                  id="with-you"
                  placeholder="Who is coming with you"
                />
              </div>
              <div>
                <input
                  type="text"
                  class="form-control"
                  name="note"
                  id="note"
                  placeholder="Extra note for us"
                />
              </div>
              <div>
                <select name="guest" class="form-control">
                  <option disabled="disabled" selected>Music preference</option>
                  <option>Pop</option>
                  <option>Rock</option>
                  <option>Jazz</option>
                </select>
              </div>

              <div class="submit-area">
                <button type="submit" class="theme-btn">RSVP</button>
                <div id="c-loader">
                  <i class="ti-reload"></i>
                </div>
              </div>
              <div class="clearfix error-handling-messages">
                <div id="success">Thank you</div>
                <div id="error">
                  Error occurred while sending email. Please try again later.
                </div>
              </div>
            </form>
          </div>
        </div>
        <div class="back-shape">
          <img src="/assets/images/rsvp/rsvp-bg.png" />
        </div>
        <div class="shape-1 wow fadeInLeftSlow" data-wow-duration="2000ms">
