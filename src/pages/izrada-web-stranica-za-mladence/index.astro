---
import FashionTitle from "../../components/ui/FashionTitle.astro"
import Header from "../../components/ui/Header.astro"
import PriceList from "../../components/ui/PriceList.astro"
import TitleAndText from "../../components/ui/TitleAndText.astro"
import Layout from "../../layouts/Layout.astro"

const SEO = {
  title: "Uzimam | izrada web stranice za mladence",
  description:
    "Izradite magičnu web stranicu za vaše vjenčanje! Slike sa vjenčanja, izbor dizajna, informacije za goste, online RSVP, odbrojavanje i sve što trebate za vaš poseban dan na jednom mjestu.",
}
---

<Layout title={SEO.title} description={SEO.description}>
  <Header
    title="Web stranica za mladence"
    subtitle="Stranica koju ćete uvijek i svugdje pokazivati svima"
  />
  <TitleAndText
    title="Uspomene i trajni zapis na jednom mjestu"
    text=" Prije vjenčanja, stranica služi kao pozivnica i izvor svih informacija za vaše goste. Nakon vjenčanja, stranica postaje trajni zapis vaših uspomena, uvijek dostupna za dijeljenje s obitelji i prijateljima, čuvajući vaše nezaboravne trenutke za buduće generacije."
  />
  <FashionTitle title="Izaberi jedan od 4 dizajna" />
  <!-- 4 dizajna start -->
  <section class="wpo-category-section-s3 section-padding">
    <h2 class="hidden">Category</h2>
    <div class="container">
      <div class="wpo-category-wrap">
        <div class="row">
          <div class="col-xl-3 col-lg-6 col-md-6 col-12">
            <div
              class="wpo-category-item wow fadeInUp"
              data-wow-duration="1000ms"
            >
              <div class="wpo-category-img">
                <div class="wpo-category-img-inner">
                  <img src="assets/images/category/1.jpg" alt="" />
                </div>
              </div>
              <div class="wpo-category-text">
                <a href="/izrada-web-stranica-za-mladence/dizajn-1">Dizajn 1</a>
              </div>
            </div>
          </div>
          <div class="col-xl-3 col-lg-6 col-md-6 col-12">
            <div
              class="wpo-category-item wow fadeInUp"
              data-wow-duration="1200ms"
            >
              <div class="wpo-category-img">
                <div class="wpo-category-img-inner">
                  <img src="assets/images/category/2.jpg" alt="" />
                </div>
              </div>
              <div class="wpo-category-text">
                <a href="/izrada-web-stranica-za-mladence/dizajn-2">Dizajn 2</a>
              </div>
            </div>
          </div>
          <div class="col-xl-3 col-lg-6 col-md-6 col-12">
            <div
              class="wpo-category-item wow fadeInUp"
              data-wow-duration="1400ms"
            >
              <div class="wpo-category-img">
                <div class="wpo-category-img-inner">
                  <img src="assets/images/category/3.jpg" alt="" />
                </div>
              </div>
              <div class="wpo-category-text">
                <a href="/izrada-web-stranica-za-mladence/dizajn-3">Dizajn 3</a>
              </div>
            </div>
          </div>
          <div class="col-xl-3 col-lg-6 col-md-6 col-12">
            <div
              class="wpo-category-item wow fadeInUp"
              data-wow-duration="1600ms"
            >
              <div class="wpo-category-img">
                <div class="wpo-category-img-inner">
                  <img src="assets/images/category/4.jpg" alt="" />
                </div>
              </div>
              <div class="wpo-category-text">
                <a href="/izrada-web-stranica-za-mladence/dizajn-4">Dizajn 4</a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <PriceList />
</Layout>
